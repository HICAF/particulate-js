<div id="classes" class="sidebox">
  <div class="hd">
    <h3 class="no-toc">Classes</h3>
  </div>
  <div class="bd">
    <ul>
      {{#classes}}
        <li><a href="{{../projectRoot}}classes/{{name}}.html">{{displayName}}</a></li>
      {{/classes}}
    </ul>
  </div>
</div>

{{#if methods}}
<div id="methods-on-page" class="sidebox on-page">
  <div class="hd">
    <h3 class="no-toc">{{name}} Methods</h3>
  </div>
  <div class="bd">
    <ul>
      {{#methods}}
        <li><a href="#methods_{{name}}">{{name}}
          {{#if access}}<code class="access">{{access}}</code>{{/if}}
        </a></li>
      {{/methods}}
    </ul>
  </div>
</div>
{{/if}}

